{
  "name": "llm-kg",
  "private": true,
  "version": "0.0.0",
  "packageManager": "pnpm@9.12.0",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "lint": "eslint . --ext .ts,.tsx,.js,.vue --max-warnings=0",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.vue --fix",
    "format": "prettier . --write",
    "stylelint": "stylelint '**/*.{css,scss,vue}'",
    "typecheck": "pnpm -r run typecheck || true",
    "test": "pnpm -r run test || true",
    "test:ci": "pnpm -r run test:ci || pnpm -r run test || true",
    "build": "cd admin-dashboard && npm run build",
    "dev": "cd admin-dashboard && npm run dev"
  },
  "dependencies": {
    "vue": "^3.5.21",
    "vue-router": "^4.5.1",
    "element-plus": "^2.11.2",
    "@element-plus/icons-vue": "^2.3.2",
    "axios": "^1.11.0",
    "echarts": "^5.6.0",
    "vue-echarts": "^7.0.3",
    "pinia": "^2.3.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^10.4.0",
    "postcss-html": "^1.7.0",
    "prettier": "^3.6.2",
    "stylelint": "^16.8.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^36.0.1"
  }
}