{
  "name": "user-portal",
  "version": "1.0.0",
  "description": "企业内部门户系统 (员工端)",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run typecheck",
    "preview": "vite preview",
    "mock": "node mock-server.cjs",
    "dev:full": "concurrently \"npm run mock\" \"npm run dev\"",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "axios": "^1.6.2",
    "echarts": "^5.6.0",
    "element-plus": "^2.4.4",
    "pinia": "^2.1.7",
    "vue": "^3.4.0",
    "vue-echarts": "^7.0.3",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "eslint": "^9.34.0",
    "eslint-plugin-vue": "^10.4.0",
    "express": "^4.21.2",
    "globals": "^16.3.0",
    "happy-dom": "^18.0.1",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "sass-embedded": "^1.91.0",
    "typescript": "^5.3.3",
    "unplugin-auto-import": "^0.17.2",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^7.1.3",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.6"
  }
}